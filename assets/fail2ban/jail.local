[sshd]
enabled = true
port = 50683
filter = sshd
bantime = 120
findtime = 600
maxretry = 3
action = iptables[name=SSH, port=50683, protocol=tcp]

[http-get-dos]
enabled = true
port = http,https
filter = http-get-dos
logpath = /var/log/apache2/access.log
maxretry = 300
findtime = 300
bantime = 600
action = iptables-multiport[name=HTTP, port="http,https", protocol=tcp]
